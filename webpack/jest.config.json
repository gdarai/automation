{
    "testRegex": "((\\.|/*.)(spec))\\.tsx?$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|sass)$": "<rootDir>/test/styleMock.js",
      "^app(.*)$": "<rootDir>/src$1"
    },
    "setupFilesAfterEnv": [
        "./setupTests.js"
    ],
    "verbose": true,
    "transform": {
        "^.+\\.[t|j]sx?$": "babel-jest"
    }
}
